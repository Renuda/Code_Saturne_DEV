<?xml version="1.0" encoding="utf-8"?><Code_Saturne_GUI case="CAS1" solver_version="6.0-patch" study="STUDY_TEST_for_mesh" version="2.0">
  <additional_scalars/>
  <analysis_control>
    <output>
      <listing_printing_frequency>1</listing_printing_frequency>
      <mesh id="-1" label="Fluid domain" type="cells">
        <all_variables status="on"/>
        <location>all[]</location>
        <writer id="-1"/>
      </mesh>
      <mesh id="-2" label="Boundary" type="boundary_faces">
        <all_variables status="on"/>
        <location>all[]</location>
        <writer id="-1"/>
