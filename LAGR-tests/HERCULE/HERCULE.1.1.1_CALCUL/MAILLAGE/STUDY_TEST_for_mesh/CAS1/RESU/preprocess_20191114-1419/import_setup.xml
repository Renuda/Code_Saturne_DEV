      <property choice="constant" label="LamVisc" name="molecular_viscosity">
        <initial_value>1.83e-05</initial_value>
        <listing_printing status="off"/>
        <postprocessing_recording status="off"/>
      </property>
    </fluid_properties>
    <notebook/>
  </physical_properties>
  <solution_domain>
    <extrusion/>
    <faces_cutting status="off"/>
    <joining/>
    <mesh_smoothing status="off"/>
    <meshes_list>
      <mesh name="avecpointe.des"/>
    </meshes_list>
    <periodicity/>
    <thin_walls/>
    <volumic_conditions>
      <zone groundwater_law="off" head_losses="off" id="1" initialization="on" label="all_cells" mass_source_term="off" momentum_source_term="off" porosity="off" scalar_source_term="off" thermal_source_term="off">all[]</zone>
    </volumic_conditions>
  </solution_domain>
